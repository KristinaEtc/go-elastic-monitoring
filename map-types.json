{
    "template": "global_logs*",
    "settings": {
        "number_of_shards": 1
        },
        "mappings" : {
            "table-info" : {
                "_source" : { "enabled" : true },
                   "properties" : {
                        "id" : { "type" : "string", "index" : "not_analyzed" },
                        "pid" : {"type" : "long"},
                        "process_short" : {"type": "string", "index":"not_analyzed"},
                        "process" : {"type": "string"},
                        "tid" : {"type": "long"},
                        "subtype" : {"type": "string"},
                        "type" : {"type": "string"},
                        "user": {"type" : "string"},
                        "utc" : {"type": "date"  },
                        "type": {"type" : "string", "index": "not_analyzed"},
                        "computer": {"type": "string", "index": "not_analyzed"}
            }
        }
    }
}
